rmove 0.25 0.3
  text \tex{$n_f = 3$}

set color black
amove 8.8 8.8
box 0.75 0.3 fill  rgba255(255,166,0,100) nostroke
set hei 0.4
rmove 1.0 -0.00
 text \tex{$|t^\star|\in [\frac 12, 2] T^2$}
amove 8.825 8.95
