size 15 11
set font psncsi
set lwidth 0.04
set cap round

ym = 1.7
xm = 5.2

amove 2 2
set texlabels 1
begin graph
  size 6 8 fullsize

  xlabels hei 0.5
  xticks length 0.25
  ylabels hei 0.5
  yticks length 0.25

  key compact offset 6.5 1. hei 0.4  nobox

  xaxis min .8 max xm  dticks 1.
  yaxis min -.2 max ym  dticks .5

  ytitle "$\eta(T) / s_{_{\rm lQCD}}$" hei 0.6 dist 0.4
  xplaces 1 2 3 4 5
  xsubticks off
  yplaces 0 .5 1 1.5

  data "../data/mey1.dat" d1=c1,c2
  data "../data/mey1.dat" d2=c1,c3
  data "../data/mey1.dat" d3=c1,c2

  data "../data/mey2.dat" d4=c1,c2
  data "../data/mey2.dat" d5=c1,c3
  data "../data/mey2.dat" d6=c1,c2

  data "../data/bor1.dat" d7=c1,c2
  data "../data/bor1.dat" d8=c1,c3
  data "../data/bor1.dat" d9=c1,c2

  let d10 = 1./(4.*PI) from 1. to 5. step 0.1
  let d11 = .5/(4.*PI) from 1. to 5. step 0.1

  data "../data/Eta_s, RENORM [nf=0].dat" d31=c1,c2
  data "../data/Eta_s, RENORM [nf=0].dat" d32=c1,c3
  data "../data/Eta_s, RENORM [nf=0].dat" d33=c1,c4
  data "../data/Eta_s, RENORM [nf=0].dat" d34=c1,c5
  data "../data/Eta_s, RENORM [nf=0].dat" d35=c1,c6
  data "../data/Eta_s, RENORM [nf=0].dat" d36=c1,c7

  data "../data/Eta_s, [nll, nf=0].dat" d81=c1,c2
  data "../data/Eta_s, [nll, nf=0].dat" d82=c1,c3

  data "../data/Eta_s, [from wtr].dat" d71=c1,c2
  data "../data/Eta_s, [from wtr].dat" d72=c1,c3

  !data "wtr eos.csv" d91=c1,c2
  !data "wtr eos.csv" d92=c1,c3

  ! 1/4PI
  !fill d11,d10 color rshade3
  !d10 lstyle 1 lwidth 0.04

  ! pQCD
  fill d81,d82 color rshade2
  fill d32,d33 color rgba255(255,166,0,80)
  d31 smooth color crimson lwidth 0.10 lstyle 1
  d34 smooth color crimson lwidth 0.05 lstyle 52
  d35 smooth color crimson lwidth 0.03 lstyle 2
  d36 smooth color crimson lwidth 0.03 lstyle 2

  ! LATTICE DATA:
  d3    err d2 color .6 lwidth 0.06 errwidth 0.0001
  d1 marker fcircle    color #0057AE lwidth 0.03 msize .25 !key "Mey1"
  d6    err d5 color .6 lwidth 0.06 errwidth 0.0001 
  d4 marker fsquare    color #0057AE lwidth 0.03 msize .2  !key "May2"
  d9    err d8 color .6 lwidth 0.06 errwidth 0.0001 
  d7 marker ftriangle  color #0057AE lwidth 0.03 msize .3  !key "Bor"

  ! wtr^-1
  !d91 smooth color orange lwidth 0.1 lstyle 1
  !d92 smooth color orange lwidth 0.1 lstyle 4

end graph

set color black
set hei 0.4
set font psncsr
begin key
  compact
  nobox
  position tl
  offset 6.6 2.41
  text "" color .6 lwidth .05
  text "" color .6 lwidth .05
  text "" color .6 lwidth .05
end key

!set color orange
!amove 4 7
!rline 0 -1.3 arrow end
!rmove 0.3 0.8
!text renorm
!let ym = 1.5

set color black
amove 8 2
begin graph
  size 6 8 fullsize

  xlabels font psncsr hei 0.5
  xticks length 0.25
  ylabels font psncsr hei 0.5
  yticks length 0.25

  key offset 3.7 3.9 hei 0.52  nobox

  xaxis min 0.8 max xm dticks 1. 
  yaxis min -.2 max ym  dticks .5
  xplaces 1 2 3 4 5
  ynames "" "" "" "" 
  xsubticks off

  let d10 = 1./(4.*PI) from 1. to 5. step 0.1
  let d11 = .5/(4.*PI) from 1. to 5. step 0.1

  ! Niemi's params
  let d31 = 0.227*x+(0.120-.227) from 1. to 10. step .1
  let d32 = .16                  from 1. to 10. step .1
  let d34 = 0.101*x+(0.120-.101) from 1. to 10. step .1

  data "../data/Eta_s, RENORM [nf=3].dat" d21=c1,c2
  data "../data/Eta_s, RENORM [nf=3].dat" d22=c1,c3
  data "../data/Eta_s, RENORM [nf=3].dat" d23=c1,c4
  data "../data/Eta_s, RENORM [nf=3].dat" d24=c1,c5
  data "../data/Eta_s, RENORM [nf=3].dat" d25=c1,c6

  ! 1/(4PI):
  !d10 lstyle 1 lwidth 0.04
  !fill d11,d10 color rshade3

  ! pQCD:
  fill d22,d23 color  rgba255(255,166,0,80) !bisque
  d21 smooth color crimson lwidth 0.10 lstyle 1
  !d24 smooth color crimson lwidth 0.05 lstyle 52
  d25 smooth color crimson lwidth 0.03 lstyle 2
  d24 smooth color crimson lwidth 0.03 lstyle 2

  ! Niemi:
  d31 lstyle 22 lwidth .06 color #0057AE
  d32 lstyle 22 lwidth .06 color #0057AE
  d34 lstyle 22 lwidth .06 color #0057AE

end graph

set color black
set hei 0.4
set font psncsr
begin key
  compact
  nobox
  position tl
  offset .6 1.3
  text "HTL everywhere"  color crimson lwidth 0.05 lstyle 52
  text " [?]  -  hydro" line color #0057AE lstyle 22 lwidth 0.06
  text " [?]" marker fcircle  color #0057AE lwidth 0.0000 msize .25
  text " [?]" marker fsquare  color #0057AE lwidth 0.0000 msize .2
  text " [?]" marker ftriangle color #0057AE lwidth 0.0000 msize .3
end key


set hei 0.5
set font psncsr
amove 7.5 0.6
text \tex{$T/T_c$}
set hei .4
amove 10.3 6.7
text \setfont{plsym2}\char{137}
set font psncsr
rmove .3 0
text lQCD

